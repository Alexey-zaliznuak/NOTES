generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @default(autoincrement()) @id
  name  String
  email String  @unique
  nottes Note[]
}

model Note {
  id        Int      @default(autoincrement()) @id
  title     String
  text      String
  author    User    @relation(fields: [authorId], references: [id], onDelete: Cascade)
  authorId  Int
}
